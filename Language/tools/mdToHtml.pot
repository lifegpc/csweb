msgctxt "TRANSLATOR"
msgid "lifegpc"
msgstr ""

msgctxt "TITLE"
msgid "Markdown to HTML"
msgstr ""

msgctxt "DESC"
msgid "A tool to convert GitHub Flavored Markdown to HTML."
msgstr ""

msgctxt "VER"
msgid "The version of <library>: "
msgstr ""

msgctxt "SOPTIONS"
msgid "Show options"
msgstr ""

msgctxt "HOPTIONS"
msgid "Hide options"
msgstr ""

msgctxt "OPT1"
msgid "Include a <attr> attribute on all block elements."
msgstr ""

msgctxt "OPT2"
msgid "Render <ele> elements as hard line breaks."
msgstr ""

# <type*> are type's name
msgctxt "OPT3"
msgid "Render raw HTML and unsafe links (<type1>, <type2>, <type3>, and <type4>, except for <type5>, <type6>, <type7>, or <type8> mime types). By default, raw HTML is replaced by a placeholder HTML comment. Unsafe links are replaced by empty strings."
msgstr ""

msgctxt "OPT4"
msgid "Render <ele> elements as spaces."
msgstr ""

msgctxt "OPT5"
msgid "Validate UTF-8 in the input before parsing, replacing illegal sequences with the replacement character U+FFFD."
msgstr ""

msgctxt "OPT6"
msgid "Convert straight quotes to curly, --- to em dashes, -- to en dashes."
msgstr ""

# <eg1> and <eg2> are examples
msgctxt "OPT7"
msgid "Use GitHub-style <eg1> tags for code blocks instead of <eg2>."
msgstr ""

msgctxt "OPT8"
msgid "Be liberal in interpreting inline HTML tags."
msgstr ""

msgctxt "OPT9"
msgid "Parse footnotes."
msgstr ""

msgctxt "OPT10"
msgid "Only parse strikethroughs if surrounded by exactly 2 tildes. Gives some compatibility with <library>."
msgstr ""

msgctxt "OPT11"
msgid "Use style attributes to align table cells instead of align attributes."
msgstr ""

msgctxt "OPT12"
msgid "Include the remainder of the info string in code blocks in a separate attribute."
msgstr ""
