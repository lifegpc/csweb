# You can use , to separate two names
msgctxt "TRANSLATOR"
msgid "lifegpc"
msgstr "lifegpc"

msgctxt "TITLE"
msgid "Markdown to HTML"
msgstr "Markdown 转 HTML"

msgctxt "DESC"
msgid "A tool to convert GitHub Flavored Markdown to HTML."
msgstr "一个将 GitHub 风格的 Markdown 转换为 HTML 的工具。"

msgctxt "VER"
msgid "The version of <library>: "
msgstr "<library> 版本： "

msgctxt "SOPTIONS"
msgid "Show options"
msgstr "显示选项"

msgctxt "HOPTIONS"
msgid "Hide options"
msgstr "隐藏选项"

msgctxt "AOPTIONS"
msgid "Available options:"
msgstr "可用选项："

msgctxt "OPT1"
msgid "Include a <attr> attribute on all block elements."
msgstr "在所有区块元素上新增一个属性：<attr>。"

msgctxt "OPT2"
msgid "Render <ele> elements as hard line breaks."
msgstr "将 <ele> 元素渲染成硬换行。"

# <type*> are type's name
msgctxt "OPT3"
msgid ""
"Render raw HTML and unsafe links (<type1>, <type2>, <type3>, and <type4>, "
"except for <type5>, <type6>, <type7>, or <type8> mime types). By default, "
"raw HTML is replaced by a placeholder HTML comment. Unsafe links are "
"replaced by empty strings."
msgstr ""
"渲染原始的 HTML 和不安全的链接（<type1>、<type2>、<type3> 和 <type4>，互联网"
"媒体类型为 <type5>、<type6>、<type7> 或 <type8> 的除外）。 默认情况下，原始"
"的 HTML 将会被替换为 HTML 注释。 不安全的链接将会被替换为空字符串。"

msgctxt "OPT4"
msgid "Render <ele> elements as spaces."
msgstr "将 <ele> 元素渲染成空格。"

msgctxt "OPT5"
msgid ""
"Validate UTF-8 in the input before parsing, replacing illegal sequences with "
"the replacement character U+FFFD."
msgstr "在解析前验证输入，非法序列会被 U+FFFD 替换。"

msgctxt "OPT6"
msgid "Convert straight quotes to curly, --- to em dashes, -- to en dashes."
msgstr ""
"将直引号(straight quotes)转换为弯引号(curly quotes)，将 --- 转换为长双引号"
"(em dashes)，将 -- 转换为短双引号(en dashes)。"

# <eg1> and <eg2> are examples
msgctxt "OPT7"
msgid "Use GitHub-style <eg1> tags for code blocks instead of <eg2>."
msgstr "使用 GitHub 风格的代码块标签 <eg1> 替代 <eg2>。"

msgctxt "OPT8"
msgid "Be liberal in interpreting inline HTML tags."
msgstr "自由解释内联 HTML 标签。"

msgctxt "OPT9"
msgid "Parse footnotes."
msgstr "解析脚注。"

msgctxt "OPT10"
msgid ""
"Only parse strikethroughs if surrounded by exactly 2 tildes. Gives some "
"compatibility with <library>."
msgstr ""
"只有在被 2 个波浪号包围时解析为删除线。可以与 <library> 保持一定的兼容性。"

msgctxt "OPT11"
msgid "Use style attributes to align table cells instead of align attributes."
msgstr "使用 style 属性来对齐表格元素而不是 align 属性。"

msgctxt "OPT12"
msgid ""
"Include the remainder of the info string in code blocks in a separate "
"attribute."
msgstr "将剩余的信息包含在代码块的一个单独属性中。"

msgctxt "SEXT"
msgid "Show extensions"
msgstr "显示扩展"

msgctxt "HEXT"
msgid "Hide extensions"
msgstr "隐藏扩展"

msgctxt "AEXT"
msgid "Available extensions:"
msgstr "可用扩展："

msgctxt "INP"
msgid "Input:"
msgstr "输入："

msgctxt "UF"
msgid "Use file"
msgstr "使用文件"

msgctxt "UT"
msgid "Use textbox"
msgstr "使用文本框"

msgctxt "CF"
msgid "Choose file"
msgstr "选择文件"

msgctxt "OUT"
msgid "Output:"
msgstr "输出："

msgctxt "CON"
msgid "Convert"
msgstr "转换"

msgctxt "CPCLIP"
msgid "Copy to clipboard"
msgstr "复制到剪贴板"

msgctxt "RDCLIP"
msgid "Read from clipboard"
msgstr "从剪贴板读取"
