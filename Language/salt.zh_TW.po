# Dear translator: place your name here if you like.
# You can use , to separate two names
msgctxt "TRANSLATOR"
msgid "lifegpc"
msgstr "lifegpc"

msgctxt "TITLE"
msgid "Password generator"
msgstr "密碼生成器"

msgctxt "PASS"
msgid "Your Password:"
msgstr "你的密碼："

msgctxt "SALT"
msgid "Salt"
msgstr "鹽"

# Please do not translate <salt>.
msgctxt "SALT2"
msgid "<salt> (such as service name or website's host name):"
msgstr "<salt>（例如服務名稱或者網站域名）："

msgctxt "HASHF"
msgid "Hash Function"
msgstr "雜湊函式"

# Please do not translate <hashf>.
msgctxt "HASHF2"
msgid ""
"<hashf> (If the result is too long, try to select another one from the list):"
msgstr "<hashf>（如果結果太長，請嘗試選擇列表中的其他函式）："

msgctxt "PUTSALT"
msgid "Put the salt before the password."
msgstr "將鹽放在密碼前。"

# Please do not translate <base64>.
msgctxt "NOBASE"
msgid "Do not use <base64>."
msgstr "不要使用<base64>。"

msgctxt "GEN"
msgid "Generate"
msgstr "生成"

msgctxt "CPCLIP"
msgid "Copy to clipboard"
msgstr "複製到剪貼簿"

msgctxt "RESULT"
msgid "Result:"
msgstr "結果："

msgctxt "UKNHASH"
msgid "Unknown hash function."
msgstr "未知的雜湊函式。"

msgctxt "SOURCE"
msgid "Source code:"
msgstr "原始碼："

msgctxt "THIRDLIB"
msgid "Third party library:"
msgstr "第三方庫："

msgctxt "INFO"
msgid ""
"The code was compiled by <name>. If you want to compile, make sure enable "
"\"process_common_js_modules\", set \"module_resolution\" to \"NODE\" and set "
"\"compilation_level \" to \"ADVANCED\"."
msgstr ""
"代碼使用<name>編譯。如果要編譯，請確保啟用了 process_common_js_modules，將 "
"module_resolution 的值設為NODE並將 compilation_level 設為 ADVANCED。"
